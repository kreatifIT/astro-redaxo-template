---
import { getAllProperties } from '@helpers/properties-management/server-properties';

const allProperties = getAllProperties(Astro);
---

<script define:vars={{ allProperties }}>
    //@ts-ignore
    window._properties = allProperties;
    //@ts-ignore
    window.getProperty = (key) => {
        //@ts-ignore
        return window._properties[key];
    };
</script>
