---
import type { Article } from '@adapters/redaxo/@types';
import type { ContentPageProps } from '@helpers/@types';
import Layout from '@layouts/Layout.astro';
import ModuleList from '@layouts/ModuleList.astro';

interface Props extends ContentPageProps {
    data: Article;
}

const { data: article } = Astro.props as Props;
---

<Layout>
    <Fragment slot="head"></Fragment>
    <ModuleList article={article} />
</Layout>
