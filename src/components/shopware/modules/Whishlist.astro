---
import { getShopwareInstance } from '@helpers/properties-management/server-properties';
import { getWishlistProducts } from '@shopware-pwa/shopware-6-client';
import Whistlist from '../utils/Whishlist';

const contextInstance = getShopwareInstance(Astro);
const whishlist = await getWishlistProducts({}, contextInstance).catch(
    (error) => {},
);
---

<Whistlist
    client:visible
    contextToken={contextInstance.config.contextToken}
    swLangId={contextInstance.config.languageId}
    whishlist={whishlist}
/>
