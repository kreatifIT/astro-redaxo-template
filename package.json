        "jsdom": "^22.1.0",
        "postcss-nesting": "^12.0.1",
        "prettier": "^3.0.1",
        "prettier-plugin-astro": "^0.11.0",
        "prettier-plugin-tailwindcss": "^0.4.1",
        "sass": "^1.65.1",
        "svgtofont": "^4.0.0",
        "tailwindcss-hyphens": "^0.1.0",
        "ttf2woff": "^3.0.0",
        "ttf2woff2": "^5.0.0",
        "wrangler": "^3.5.0"
    }
