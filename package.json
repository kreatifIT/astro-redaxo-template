        "format": "prettier --write .",
        "icon-font": "svgtofont --sources ./src/icons --output ./public/fonts/iconFont --fontName iconFont",
        "convert-font": "node scripts/convert-font.cjs $fontName",
        "generate-favicon": "node scripts/favicon-manifest-generator.cjs"
    },
    "dependencies": {
        "@astrojs/cloudflare": "^6.6.2",
        "@astrojs/partytown": "^1.2.3",
        "@astrojs/preact": "^2.2.2",
        "@astrojs/tailwind": "^4.0.0",
        "@fancyapps/ui": "^5.0.22",
        "@nanostores/preact": "^0.5.0",
        "@splidejs/react-splide": "^0.7.12",
        "@splidejs/splide-extension-video": "^0.8.0",
        "astro": "^2.10.4",
        "astro-seo": "^0.7.5",
        "astro-seo-schema": "^2.2.0",
        "dotenv": "^16.3.1",
        "graphql": "^16.7.1",
        "graphql-tag": "^2.12.6",
        "mapbox-gl": "^2.15.0",
        "nanostores": "^0.9.3",
        "preact": "^10.16.0",
        "react-map-gl": "^7.1.3",
        "redaxo-adapter": "^0.0.29",
        "tailwindcss": "^3.3.3",
        "video.js": "^8.3.0",
        "videojs-youtube": "^3.0.1"
